{
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "es2016",
    "lib": ["esnext"],
    "module": "commonjs",
    "resolveJsonModule": true,
    "sourceMap": true,
    "outDir": "dist",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  }
}
